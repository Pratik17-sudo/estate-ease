{
    "version": 2,
    "builds": [
        {
            "src": "client/",
            "use": "@vercel/static-build"
        },
        {
            "src": "api/index.js",
            "use": "@vercel/node"
        },
        {
            "src": "socket/app.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "api/index.js"
        },
        {
            "src": "/socket/(.*)",
            "dest": "socket/app.js"
        },
        {
            "src": "/(.*)",
            "dest": "client/$1"
        }
    ]
}